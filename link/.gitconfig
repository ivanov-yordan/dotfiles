[credential]
	helper = cache

[core]
	editor=vim
	autocrlf=input
	eol=lf

[color]
	ui = true
[merge]
	tool = meld
[diff]
	tool = meld


[push]
	default = matching
[pull]
	rebase = true

[remote "upstream-prs"]
	fetch = +refs/pull/*/head:refs/remotes/upstream/pr/*
